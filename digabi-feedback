### BEGIN INIT INFO
# Provides:          digabi-feedback
# Required-Start:    $remote_fs $syslog
# Required-Stop:     $remote_fs $syslog
# Default-Start:     2 3 4 5
# Default-Stop:      0 1 6
# Short-Description: Collect device information at boot
# Description:       Enable service provided by daemon.
### END INIT INFO

PATH=/bin:/usr/bin:/sbin:/usr/sbin
DESC="digabi device information collector"
NAME=digabi-feedback
DAEMON="/usr/bin/digabi collect-feedback"
PIDFILE=/var/run/digabi-feedback.pid
SCRIPTNAME=/etc/init.d/"$NAME"

# FIXME !!!!!!

. /lib/lsb/init-functions
case "$1" in
start)	log_daemon_msg "Starting digabi-feedback" "digabi-feedback"
	${DAEMON}
        log_end_msg $?
	;;
stop)	log_daemon_msg "Stopping digabi-feedback" "digabi-feedback"
	RETVAL="0"
        log_end_msg $RETVAL
        ;;
*)	log_action_msg "Usage: /etc/init.d/digabi-feedback {start|stop}"
        exit 2
        ;;
esac
exit 0
